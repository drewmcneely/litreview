@Article{ruan-ding_kinematics,
  author =       "Sipu Ruan and Jianzhong Ding and Qianli Ma and Gregory
                 S. Chirikjian",
  title =        "The Kinematics of Containment for N-Dimensional
                 Ellipsoids",
