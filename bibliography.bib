@Article{miolane-pennec_computing,
  author =       "Nina Miolane and Xavier Pennec",
  title =        "Computing Bi-Invariant Pseudo-Metrics on Lie Groups
                 for Consistent Statistics",
  keywords =     "Group Theory ; Signal and Image Processing ;
                 Mathematics ; Medical Imaging ; Computer Science ;
                 pseudo-Riemannian ; Lie group ; Lie algebra ;
                 statistics, geometric statistics, want to read",
  abstract =     "In computational anatomy, organ's shapes are often
                 modeled as deformations of a reference shape, i.e., as
                 elements of a Lie group. To analyze the variability of
                 the human anatomy in this framework, we need to perform
                 statistics on Lie groups. A Lie group is a manifold
                 with a consistent group structure. Statistics on
                 Riemannian manifolds have been well studied, but to use
                 the statistical Riemannian framework on Lie groups, one
                 needs to define a Riemannian metric compatible with the
                 group structure: a bi-invariant metric. However, it is
                 known that Lie groups, which are not a direct product
                 of compact and abelian groups, have no bi-invariant
                 metric. However, what about bi-invariant
                 pseudo-metrics? In other words: could we remove the
                 assumption of the positivity of the metric and obtain
                 consistent statistics on Lie groups through the
                 pseudo-Riemannian framework? Our contribution is
                 two-fold. First, we present an algorithm that
                 constructs bi-invariant pseudo-metrics on a given Lie
                 group, in the case of existence. Then, by running the
                 algorithm on commonly-used Lie groups, we show that
                 most of them do not admit any bi-invariant (pseudo-)
                 metric. We thus conclude that the (pseudo-) Riemannian
                 setting is too limited for the definition of consistent
                 statistics on general Lie groups.",
  address =      "Basel",
  copyright =    "Copyright MDPI AG 2015",
  ISSN =         "1099-4300",
  journal =      "Entropy (Basel, Switzerland)",
  language =     "eng",
  number =       "4",
  pages =        "1850--1881",
  publisher =    "MDPI AG",
  volume =       "17",
  year =         "2015",
}

@Article{turaga-veeraraghavan_statistical,
  author =       "P Turaga and A Veeraraghavan and A Srivastava and R
                 Chellappa",
  title =        "Statistical Computations on Grassmann and Stiefel
                 Manifolds for Image and Video-Based Recognition",
  keywords =     "Manifolds ; Geometry ; Shape ; Face recognition ;
                 Computational modeling ; feature representation ;
                 Humans ; statistical models ; Grassmann ; Data models ;
                 Image and video models ; Stiefel ; Usage ; Research ;
                 Machine vision ; Image processing ; Kernel functions ;
                 Mathematical optimization, geometric statistics, want
                 to read",
  abstract =     "In this paper, we examine image and video-based
                 recognition applications where the underlying models
                 have a special structure-the linear subspace structure.
                 We discuss how commonly used parametric models for
                 videos and image sets can be described using the
                 unified framework of Grassmann and Stiefel manifolds.
                 We first show that the parameters of linear dynamic
                 models are finite-dimensional linear subspaces of
                 appropriate dimensions. Unordered image sets as samples
                 from a finite-dimensional linear subspace naturally
                 fall under this framework. We show that an inference
                 over subspaces can be naturally cast as an inference
                 problem on the Grassmann manifold. To perform
                 recognition using subspace-based models, we need tools
                 from the Riemannian geometry of the Grassmann manifold.
                 This involves a study of the geometric properties of
                 the space, appropriate definitions of Riemannian
                 metrics, and definition of geodesics. Further, we
                 derive statistical modeling of inter and intraclass
                 variations that respect the geometry of the space. We
                 apply techniques such as intrinsic and extrinsic
